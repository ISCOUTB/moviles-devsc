    <EditText
        android:id="@+id/editText10"
        android:layout_width="0dp"
        android:layout_height="25dp"
        android:layout_marginStart="21dp"
        android:layout_marginTop="37dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="37dp"
